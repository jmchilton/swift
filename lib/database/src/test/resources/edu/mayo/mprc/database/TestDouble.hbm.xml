<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.mayo.mprc.database">
    <class name="TestDouble" table="set">
        <id name="id" type="integer" column="set_id">
            <generator class="native"/>
        </id>
        <property name="value1" column="value1" type="edu.mayo.mprc.database.NullNanDoubleType"/>
        <property name="value2" column="value2" type="edu.mayo.mprc.database.NullNanDoubleType"/>
    </class>
</hibernate-mapping>