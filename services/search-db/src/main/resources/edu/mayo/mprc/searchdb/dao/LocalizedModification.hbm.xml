<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.mayo.mprc.searchdb.dao" default-lazy="true">
    <class name="LocalizedModification" table="localized_modification">
        <id name="id" type="integer" column="localized_modification_id">
            <generator class="native"/>
        </id>
        <many-to-one name="modSpecificity" column="mod_specificity" class="edu.mayo.mprc.unimod.ModSpecificity"/>
        <property name="position" column="position" type="integer"/>
        <property name="residue" column="residue" type="character"/>
    </class>
</hibernate-mapping>