<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.mayo.mprc.searchdb.dao" default-lazy="true">
    <class name="IdentifiedPeptide" table="identified_peptide">
        <id name="id" type="integer" column="identified_peptide_id">
            <generator class="native"/>
        </id>
        <many-to-one name="sequence" column="peptide_sequence_id" class="edu.mayo.mprc.fastadb.PeptideSequence"
                     not-null="true"/>
        <many-to-one name="modifications" column="localized_mod_list_id" class="LocalizedModList" not-null="true"/>
    </class>
</hibernate-mapping>