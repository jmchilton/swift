<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.mayo.mprc.searchdb.dao" default-lazy="true">
    <class name="IdentifiedPeptide" table="identified_peptide">
        <id name="id" type="integer" column="identified_peptide_id">
            <generator class="native"/>
        </id>
        <many-to-one name="sequence" column="sequence" class="PeptideSequence"
                     cascade="none" not-null="true"/>
        <list name="modifications" table="peptide_modification_list">
            <key column="modifications_id"/>
            <list-index column="sort_order"/>
            <many-to-many column="modification_id" class="LocalizedModification"/>
        </list>
    </class>
</hibernate-mapping>