<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.mayo.mprc.searchdb.dao">
    <class name="ProteinGroup" table="protein_group">
        <id name="id" type="integer" column="protein_group_id">
            <generator class="native"/>
        </id>
        <many-to-one name="proteinSequences" class="ProteinSequenceList" column="protein_sequences"/>
        <many-to-one name="peptideSpectrumMatches" class="PsmList" column="peptide_spectrum_matches"/>
        <property name="proteinIdentificationProbability" type="double" column="protein_identification_probability"/>
        <property name="numberOfUniquePeptides" type="integer" column="unique_peptides"/>
        <property name="numberOfUniqueSpectra" type="integer" column="unique_spectra"/>
        <property name="numberOfTotalSpectra" type="integer" column="total_spectra"/>
        <property name="percentageOfTotalSpectra" type="double" column="percentage_total_spectra"/>
        <property name="percentageSequenceCoverage" type="double" column="percentage_sequence_coverage"/>
    </class>
</hibernate-mapping>