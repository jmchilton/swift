<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.mayo.mprc.searchdb.dao" default-lazy="true">
    <class name="LocalizedModList" table="localized_mod_list">
        <id name="id" type="integer" column="localized_mod_list_id">
            <generator class="native"/>
        </id>
        <bag name="list" table="localized_mod_list_members">
            <key column="localized_mod_list_id"/>
            <many-to-many column="localized_modification_id" class="LocalizedModification"/>
        </bag>
    </class>
</hibernate-mapping>