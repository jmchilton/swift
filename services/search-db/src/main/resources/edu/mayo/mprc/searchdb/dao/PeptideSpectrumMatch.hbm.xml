<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.mayo.mprc.searchdb.dao">
    <class name="PeptideSpectrumMatch" table="peptide_spectrum_match">
        <id name="id" type="integer" column="peptide_spectrum_match_id">
            <generator class="native"/>
        </id>
        <many-to-one name="peptide" column="peptide" not-null="true"/>
        <property name="previousAminoAcid" column="previous_aa" not-null="true" type="character"/>
        <property name="nextAminoAcid" column="next_aa" not-null="true"  type="character"/>
        <property name="bestPeptideIdentificationProbability" column="best_id_probability" not-null="true" type="double"/>
        <component name="bestSearchEngineScores" class="SearchEngineScores" lazy="false">
            <property name="mascotDeltaIonScore" column="mascot_delta_ion_score"  not-null="true" type="double"/>
            <property name="mascotHomologyScore" column="mascot_homology_score"  not-null="true" type="double"/>
            <property name="mascotIdentityScore" column="mascot_identity_score"  not-null="true" type="double"/>
            <property name="mascotIonScore" column="mascot_ion_score"  not-null="true" type="double"/>
            <property name="sequestDcnScore" column="sequest_dcn_score"  not-null="true" type="double"/>
            <property name="sequestPeptidesMatched" column="sequest_peptides_matched"  not-null="true" type="double"/>
            <property name="sequestSpRank" column="sequest_sp_rank"  not-null="true" type="double"/>
            <property name="sequestSpScore" column="sequest_sp_score"  not-null="true" type="double"/>
            <property name="sequestXcorrScore" column="sequest_xcorr_score"  not-null="true" type="double"/>
            <property name="tandemHyperScore" column="tandem_hyper_score"  not-null="true" type="double"/>
            <property name="tandemLadderScore" column="tandem_ladder_score"  not-null="true" type="double"/>
        </component>
        <component name="spectrumIdentificationCounts" class="SpectrumIdentificationCounts" lazy="false">
            <property name="numberOfIdentifiedSpectra" column="total_identified_spectra" not-null="true" type="integer"/>
            <property name="numberOfIdentified1HSpectra" column="identified_1h_spectra" not-null="true" type="integer"/>
            <property name="numberOfIdentified2HSpectra" column="identified_2h_spectra" not-null="true" type="integer"/>
            <property name="numberOfIdentified3HSpectra" column="identified_3h_spectra" not-null="true" type="integer"/>
            <property name="numberOfIdentified4HSpectra" column="identified_4h_spectra" not-null="true" type="integer"/>
        </component>
        <property name="numberOfEnzymaticTerminii" column="num_enzymatic_terminii" not-null="true" type="integer"/>
    </class>
</hibernate-mapping>