<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.mayo.mprc.searchdb.dao" default-lazy="true">
    <class name="PsmList" table="peptide_spectrum_match_list">
        <id name="id" type="integer" column="peptide_spectrum_match_list_id">
            <generator class="native"/>
        </id>
        <bag name="list" table="peptide_spectrum_match_list_members">
            <key column="list_id"/>
            <many-to-many column="item_id" class="PeptideSpectrumMatch"/>
        </bag>
    </class>
</hibernate-mapping>