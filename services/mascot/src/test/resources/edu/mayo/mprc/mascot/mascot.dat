# drosophila /usr/local/mascot/unigene/drosophila/current/Dm.data
# zebrafish /usr/local/mascot/unigene/zebrafish/current/Dr.data
# barley /usr/local/mascot/unigene/barley/current/Hv.data
# rice /usr/local/mascot/unigene/rice/current/Os.data
# rat /usr/local/mascot/unigene/rat/current/Rn.data
